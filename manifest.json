{
"manifest_version": 2,
    "name": "Word Replacer",
    "description": "Replaces specified words",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}